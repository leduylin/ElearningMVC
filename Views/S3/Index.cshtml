<h2>Basic File Upload</h2>
@using (Html.BeginForm("Index",
                       "S3",
                       FormMethod.Post,
                       new { enctype = "multipart/form-data" }))
{
    <label for="file">Upload File:</label>
    <input type="file" name="file" id="file" />
    <br>
    <br>
    <input type="submit" value="Upload Image" />
    <br>
    <br>
    @ViewBag.Message
}